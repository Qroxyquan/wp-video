#: plugin-update-checker.php:222
#, php-format
msgid "The URL %s does not point to a valid plugin metadata file. "
msgstr " %s 指向的文件不是有效的插件元数据文件。 "

#: plugin-update-checker.php:228
msgid "wp_remote_get() returned an unexpected result."
msgstr "wp_remote_get() 返回了异常结果。"

#: plugin-update-checker.php:273
#, php-format
msgid "Can't to read the Version header for %s. The file does not exist."
msgstr "%s 文件中没有版本信息。文件不存在。"

#: plugin-update-checker.php:291
#, php-format
msgid ""
"Can't to read the Version header for %s. The filename is incorrect or is not "
"a plugin."
msgstr "%s 文件中没有版本信息。文件名不正确或者不是一个插件。"

#: plugin-update-checker.php:313
#, php-format
msgid "Skipping update check for %s - installed version unknown."
msgstr "跳过 %s 的更新 - 未识别的安装版本。"

#: plugin-update-checker.php:556
msgid "Check for updates"
msgstr "检查更新"

#: plugin-update-checker.php:579
msgid "no_update"
msgstr "暂无更新"

#: plugin-update-checker.php:579
msgid "update_available"
msgstr "更新可用。"

#: plugin-update-checker.php:600
msgid "This plugin is up to date."
msgstr "插件已经是最新版本。"

#: plugin-update-checker.php:602
msgid "A new version of this plugin is available."
msgstr "插件有版本更新。"

#: plugin-update-checker.php:604
#, php-format
msgid "Unknown update checker status \"%s\""
msgstr "未知的更新状态 \"%s\""

#: plugin-update-checker.php:773
msgid ""
"Failed to parse plugin metadata. Try validating your .json file with http://"
"jsonlint.com/"
msgstr "解析插件元数据失败。 尝试到 http://jsonlint.com/ 验证JSON文件。"

#: plugin-update-checker.php:785
msgid ""
"The plugin metadata file does not contain the required 'name' and/or "
"'version' keys."
msgstr "插件元数据文件中未找到‘name’或者‘version’字段。"
